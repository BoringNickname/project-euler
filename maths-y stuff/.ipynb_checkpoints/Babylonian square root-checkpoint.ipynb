    "def babylonian(xn, sqrt):\n",
    "    return (xn+sqrt/xn)/2\n",
    "\n",
    "guess = 10000\n",
    "root = 45\n",
    "\n",
    "i = 0\n",
    "while i < 100:\n",
    "    guess = babylonian(guess, root)\n",
    "#     print(guess)\n",
    "    i +=1\n",
    "\n",
    "print(guess)"
