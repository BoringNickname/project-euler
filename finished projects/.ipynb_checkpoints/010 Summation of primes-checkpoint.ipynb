   ],
   "source": [
    "n = 2000000\n",
    "i = 2\n",
    "sums = 0\n",
    "while i <= n:\n",
    "    if isPrime(i) == True:\n",
    "        sums = sums + i\n",
    "    i += 1\n",
    "print(sums)"
   ]
