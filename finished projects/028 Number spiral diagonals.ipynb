    "import numpy as np\n",
    "target = 1001\n",
    "step = 2\n",
    "dummy = 1\n",
    "i = 0\n",
    "tot = 1\n",
    "while i < int(target/2):\n",
    "    tot = tot + (step+dummy)\n",
    "    dummy = step+dummy\n",
    "#     print(dummy)\n",
    "    if np.sqrt(dummy) == (step+1):\n",
    "        i += 1\n",
    "        step += 2\n",
    "print(tot)"
   ]
